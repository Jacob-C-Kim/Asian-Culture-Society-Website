{
  "name": "acs-workspace",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "dev": "pnpm --dir apps/web exec next dev",
    "build": "pnpm --dir apps/web exec next build",
    "start": "pnpm --dir apps/web exec next start",
    "lint": "pnpm --dir apps/web exec next lint",
    "typecheck": "pnpm --dir apps/web exec tsc --noEmit",
    "sec:scan": "./tools/scan-secrets.sh",
    "sec:scan:staged": "./tools/scan-staged-secrets.sh",
    "db:generate": "pnpm -F @acs/db prisma:generate",
    "db:migrate": "pnpm -F @acs/db prisma:migrate:dev",
    "db:deploy": "pnpm -F @acs/db prisma:migrate:deploy",
    "db:seed": "pnpm -F @acs/db seed",
    "prepare": "husky"
  },
  "overrides": {
    "prismjs": "^1.30.0",
    "refractor>prismjs": "^1.30.0"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "tsx": "^4.19.0",
    "turbo": "^2.0.0"
  }
}

